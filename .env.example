# Environment Variables for Financial Advisor AI
# Copy this file to .env and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# HubSpot Configuration
HUBSPOT_API_KEY=your_hubspot_api_key_here

# Guardian JWT Secret
GUARDIAN_SECRET_KEY=your_guardian_secret_key_here

# Cloak Encryption Key (generate with: 32 |> :crypto.strong_rand_bytes() |> Base.encode64())
CLOAK_KEY=your_cloak_encryption_key_here

# Database Configuration (if needed in production)
# DATABASE_URL=your_database_url_here

# Phoenix Secret Key Base (if needed in production)
# SECRET_KEY_BASE=your_secret_key_base_here